	</plug-in>

	<plug-in className="org.apache.struts.validator.ValidatorPlugIn">
		<set-property property="pathnames"
			value="/org/apache/struts/validator/validator-rules.xml,/com/iberia/action/validator-rules-plusoft.xml,/WEB-INF/config/crm/validation.xml" />
		<set-property property="stopOnFirstError" value="false" />
	</plug-in>
</struts-config>